@font-face {
  font-family: "Graphik Web";
  src: url(<%= asset_path('graphik/Graphik-Regular-Web.eot') %>);
  src: url(<%= asset_path('graphik/Graphik-Regular-Web.eot?#iefix') %>) format("embedded-opentype"), url(<%= asset_path('graphik/Graphik-Regular-Web.woff') %>) format("woff"), url(<%= asset_path('graphik/Graphik-Regular-Web.ttf') %>) format("truetype"), url(<%= asset_path('graphik/Graphik-Regular-Web.svg#Graphik-Regular') %>) format("svg");
  font-style: normal;
  font-stretch: normal;
  font-weight: 400
}
